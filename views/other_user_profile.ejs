          <h1 class="display-4">Here's <%- data[0].name %>.  Lookit!</h1>
        </div>
        <div class="row">
          <div class="col-2">
            <img src="<%-data[0].avatar_url%>" class="img-fluid img-thumbnail rounded-circle border-dark">
          </div>
            <p class="lead"><%- data[0].bio %></p>
          <div class="col-10">

          </div>
        </div>
      </div>

      <div class="card-deck">
        <div id="search_results" class="row">

          <% for (kumquat of data) {%>

            <%- include("partials/_resource"); %>
          <% } %>
        </div>
      </div>
